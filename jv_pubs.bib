	Jointfirst = {1},
	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Number = {45},
	Pages = {19061--19066},
	Sort-Order = {5},
	Title = {A theory for the evolution of other-regard integrating proximate and ultimate perspectives},
	Url = {http://www.pnas.org/content/106/45/19061.abstract},
