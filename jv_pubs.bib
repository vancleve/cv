	File = {VanCleve.Feldman_2007_Genetics.pdf:VanCleve.Feldman_2007_Genetics.pdf:PDF},
	Institution = {Department of Biological Sciences, Stanford University, Stanford, California 94305.},
	Journal = {Genetics},
	Local-Url = {file://localhost/Users/vancleve/science/references/articles/VanCleve.Feldman_2007_Genetics.pdf},
	Month = {Jun},
	Number = {2},
